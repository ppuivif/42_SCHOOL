int	ft_isprint(int c)

->return non nonzero if c is included between space (32 value) and ~ (126 value). 
->return zero if not.
->no protection needed.

int	ft_isprint(int c)
{
	if ((c >= 32) & (c <= 126))
		return (16384);
	return (0);
}

#include <stdio.h>
#include <ctype.h>
#include <stdlib.h>

int	main(int argc, char **argv)
{
	if (argc == 2)
		printf("real_function return :\t%d\n", isprint(atoi(argv[1])));
	if (argc == 3)
		printf("ft_function return :\t%d\n", ft_isprint(atoi(argv[1])));
	return (0);
}
