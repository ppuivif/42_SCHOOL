int	ft_isalnum(int c)

->return non nonzero if c is included between a and z or A and Z or 0 and 9. 
->return zero if not.
->no protection needed.

int	ft_isalnum(int c)
{
	if (((c >= 'a') && (c <= 'z')) || ((c >= 'A') && (c <= 'Z'))
		|| ((c >= '0') && (c <= '9')))
		return (98);
	return (0);
}

#include <stdio.h>
#include <ctype.h>
#include <stdlib.h>

int	main(int argc, char **argv)
{
	if (argc == 2)
		printf("real function return :\t%d\n", isalnum(atoi(argv[1])));
	if (argc == 3)
		printf("ft_function return :\t%d\n", ft_isalnum(atoi(argv[1])));
	return (0);
}
