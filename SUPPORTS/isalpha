int	ft_isalpha(int c)

->return non nonzero if c is included between a and z or A and Z. 
->return zero if not.
->no protection needed.

int	ft_isalpha(int c)
{
	if (((c >= 'a') & (c <= 'z')) || ((c >= 'A') & (c <= 'Z')))
		return (1);
	return (0);
}

#include <ctype.h>
#include <unistd.h>
#include <stdio.h>
#include <stdlib.h>

int	main(int argc, char **argv)
{
	if (argc == 2)
		printf("real_function return :\t%d\n", isalpha(atoi(argv[1])));
	if (argc == 3)
		printf("ft_function return :\t%d\n", ft_isalpha(atoi(argv[1])));
	return (0);
}
